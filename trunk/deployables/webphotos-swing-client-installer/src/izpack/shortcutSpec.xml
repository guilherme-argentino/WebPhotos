			target="$INSTALL_PATH\javadoc\index.html"
			commandLine=""
			description="Apresenta a documentação da API do sistema (Javadoc)">
		<createForPack name="Javadoc" />
	</shortcut>
	
	<shortcut	name="Desinstalar o WebFotos"
			programGroup="yes"
			desktop="no"
			applications="no"
			startMenu="no"
			startup="no"
			target="$INSTALL_PATH\Uninstaller\uninstaller.jar"     
			commandLine=""
			iconFile="%SystemRoot%\system32\SHELL32.dll"
			iconIndex="31" 
			description="Este desinstala o WebFotos de seu sistema">
		<createForPack name="Núcleo"/>
