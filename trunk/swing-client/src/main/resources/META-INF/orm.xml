<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" 
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" 
                 version="1.0">
    <sql-result-set-mapping name="AlbumMappingSQL1Xml">
        <!-- albumID AS ID, categorias.nmcategoria AS Categoria, nmalbum AS Pauta, DtInsercao AS Data -->
        <entity-result entity-class="net.sf.webphotos.model.AlbumVO">
            <field-result column="ID" name="albumid"/>
            <field-result column="CATEGORIAID" name="categoriasVO"/>
            <field-result column="PAUTA" name="nmalbum"/>
            <field-result column="PAUTA" name="descricao"/>
            <field-result column="DATA" name="dtInsercao"/>
        </entity-result>
    </sql-result-set-mapping>
</entity-mappings>
