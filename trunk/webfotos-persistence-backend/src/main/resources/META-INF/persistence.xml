<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="jee_webphotos_pu" transaction-type="JTA">
    <jta-data-source>java:mysql_remote</jta-data-source>
    <class>br.com.guilherme.webphotos.model.CreditosVO</class>
    <class>br.com.guilherme.webphotos.model.AlbunsVO</class>
    <class>br.com.guilherme.webphotos.model.FotosVO</class>
    <class>br.com.guilherme.webphotos.model.CategoriasVO</class>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
    </properties>
  </persistence-unit>
</persistence>
